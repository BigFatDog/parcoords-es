{
  "name": "parcoord-es",
  "version": "2.2.10",
  "description": "ES6 module of Parallel Coordinates, based on d3 v5 modules",
  "main": "dist/parcoords.js",
  "module": "dist/parcoords.esm.js",
  "jsnext:main": "dist/parcoords.esm.js",
  "style": "dist/parcoords.css",
  "scripts": {
    "_prebuild": "eslint src test",
    "build": "rollup -c",
    "dev": "rollup -c rollup.config.dev.js -w",
    "watch": "rollup -c -w",
    "pretest": "npm run build",
    "test:watch": "mocha --reporter min --watch",
    "test": "mocha",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "prepare": "npm test",
    "format": "prettier --single-quote --trailing-comma es5 --write \"{src,__{tests,demo,dist}__}/**/*.{js,html}\""
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-axis": "^1.0.8",
    "d3-brush": "^1.0.4",
    "d3-collection": "^1.0.4",
    "d3-dispatch": "^1.0.3",
    "d3-drag": "^1.2.1",
    "d3-interpolate": "^1.2.0",
    "d3-scale": "^2.1.0",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "d3-transition": "^1.1.1",
    "requestanimationframe": "0.0.23",
    "sylvester-es6": "0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^4.19.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "prettier": "1.13.5",
    "rollup": "^0.60.7",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-watch": "^4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigFatDog/parcoords-es.git"
  },
  "keywords": [
    "es6",
    "d3",
    "v4",
    "ParCoords",
    "visualization",
    "parallel coordinates"
  ],
  "author": {
    "name": "Xing Yun"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BigFatDog/parcoords-es/issues"
  }
}
